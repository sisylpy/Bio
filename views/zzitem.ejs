<script>
  // var acolor = document.getElementById('articles');
  // acolor.style.color = "#dee558";
  $(document).ready(function () {


    $('.about').mouseover(function () {
      $(this).css("background-color", "#008cc3").siblings().css('background','none');
    });


    $('.about').on('click',function () {
      $(this).addClass('active').siblings().removeClass('active');

      var datalinkid = $(this).attr('data-linkedid');
      var items = $('#rightData').children();

      for(var i =0 ; i< items.length; i++) {
        var itemData = items[i].getAttribute('id');

        if ( datalinkid == itemData ){

          items[i].addClass('active').siblings().removeClass('active');
        }


      }
    })


  });

</script>
